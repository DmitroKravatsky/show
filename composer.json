        "borales/yii2-phone-input": "*",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "frostealth/yii2-aws-s3": "~2.0",
        "yiister/yii2-gentelella": "~1.0",
        "kartik-v/yii2-widget-fileinput": "@dev",
